{
  "name": "keyless",
  "version": "1.0.0",
  "description": "Keyless car rental service application",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server/server.js",
    "gulp": "NODE_OPTIONS='--max-old-space-size=4096' gulp",
    "build:css": "tailwindcss -i ./src/styles/styles.css -o ./public/css/styles.css",
    "watch:css": "tailwindcss -i ./src/styles/styles.css -o ./public/css/styles.css --watch",
    "dev": "npm-run-all --parallel start gulp watch:css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/keyless.git"
  },
  "author": "Sebastjan",
  "license": "AllCopyright Reserved",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.12.0",
    "stripe": "^16.8.0"
  },
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "browser-sync": "^3.0.2",
    "concurrently": "^8.2.2",
    "gulp": "^5.0.0",
    "gulp-postcss": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.4.10",
    "uuid": "^10.0.0"
  }
}


